        app:layout_constraintEnd_toStartOf="@+id/downButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/downButton"
        app:srcCompat="?attr/actionModeCloseDrawable" />

